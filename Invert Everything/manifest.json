{
  "manifest_version": 2,
  "name": "Invert Everything",
  "version": "1.0",
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>", "chrome://*/*" ],
      "css": ["invert.css"],
      "all_frames": true
    }
  ],

    "permissions": [ "tabs", "<all_urls>", "chrome://*/*" ]

}